<% include header %>

    <div>
    <% if(users.length > 0) { %>
        <table border="1">
            <tr><th>Name</th></tr>
            <% for (var i=0; users.length > i; i++) { %>
                <tr>
                 <td><a href="/user/?userid=<%= users[i].UserID %>"> <%= users[i].Username %></a></td>
                </tr>
            <% } %>
        </table>
    <% } else { %>
         No users are entered into the User database table.
    <% } %>
    </div>

    <div style="margin-top:10px;">
        <a href="/user/create">Create New User</a>
    </div>

<% include footer %>